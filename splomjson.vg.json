            "filter": "datum.Stop_1 == 'MEL' && datum.Stop_2 == 'PER'"
          }, {
            "filter": {
              "param": "brush"
            }
          }, {
            "filter": "datum.Flights > 0"
          }],
          "layer": [{
              "title": "Passenger Capacity (MEL - PER)",
              "mark": "point",
              "encoding": {
                "x": {
                  "field": "Flights",
                  "type": "quantitative",
                  "scale": {
                    "type": "linear"
                  }
                },
                "y": {
                  "field": "Seats",
                  "type": "quantitative",
                  "scale": {
                    "type": "linear"
                  }
                }
              }
            },
            {
              "mark": {
                "type": "rule",
                "color": "firebrick"
              },
              "encoding": {

                "y": {
                  "field": "Passengers",
                  "type": "quantitative",
                  "aggregate": "mean",
                  "title": "Passengers"
                }
              }
            },
            {
              "mark": {
                "type": "errorband",
                "extent": "stdev",
                "opacity": 0.2
              },
              "encoding": {

                "y": {
                  "field": "Passengers",
                  "type": "quantitative",
                  "title": "Passengers"
                },
                "tooltip": null
              }
            },
            {
              "transform": [{
                  "aggregate": [{
                      "op": "mean",
                      "field": "Passengers",
                      "as": "mean2"
                    },
                    {
                      "op": "stdev",
                      "field": "Passengers",
                      "as": "stdev2"
                    }
                  ]
                },
                {
                  "calculate": "'Mean Passengers: '+ format(datum.mean2, '.0f')",
                  "as": "R2"
                }
              ],
              "mark": {
                "type": "text",
                "color": "firebrick",
                "x": "width",
                "align": "right",
                "y": -5
              },
              "encoding": {
                "text": {
                  "type": "nominal",
                  "field": "R2"
                }
              }
            }
          ]
        }]


      }
    ]
  }]
}